<script>
  var yogaSlideIndex = 1;
  showYogaDivs(yogaSlideIndex);

  function plusYogaDivs(n) {
    showYogaDivs(yogaSlideIndex += n);
  }

  function showYogaDivs(n) {
    var i;
    var x = document.getElementsByClassName('yogaFlowPic');
    if (n > x.length) {yogaSlideIndex = 1}
    if (n < 1) {yogaSlideIndex = x.length}
    for (i = 0; i < x.length; i++) {
      x[i].style.display = "none";
    }
    x[yogaSlideIndex-1].style.display = "block";
  }
</script>
