<script>
  var fuelSlideIndex = 1;
  showFuelDivs(fuelSlideIndex);

  function plusFuelDivs(n) {
    showFuelDivs(fuelSlideIndex += n);
  }

  function showFuelDivs(n) {
    var i;
    var x = document.getElementsByClassName('altFuelPic');
    if (n > x.length) {fuelSlideIndex = 1}
    if (n < 1) {fuelSlideIndex = x.length}
    for (i = 0; i < x.length; i++) {
      x[i].style.display = "none";
    }
    x[fuelSlideIndex-1].style.display = "block";
  }
</script>
